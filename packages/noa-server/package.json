{
  "name": "noa-server",
  "version": "0.0.1",
  "private": true,
  "description": "A NestJS Project.",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start": "nest start",
    "start:debug": "cross-env NODE_ENV=developmtn nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compodoc": "pnpx @compodoc/compodoc -p tsconfig.json -s -o -c .compodoc.json",
    "yac": "node_modules/.bin/httpyac -e dev -o body ./yac.http"
  },
  "dependencies": {
    "@nestjs/common": "11.0.3",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.3",
    "@nestjs/event-emitter": "3.0.0",
    "@nestjs/platform-express": "11.0.0",
    "@nestjs/platform-socket.io": "11.0.3",
    "@nestjs/swagger": "11.0.2",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "11.0.3",
    "bcrypt": "5.1.1",
    "better-sqlite3": "9.6.0",
    "chalk": "4.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.6",
    "crypto-js": "4.2.0",
    "ejs": "3.1.10",
    "ioredis": "5.4.1",
    "lodash": "4.17.21",
    "mime-types": "3.0.0",
    "moment": "2.30.1",
    "nanoid": "3.3.7",
    "pako": "2.1.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "safe-stable-stringify": "2.5.0",
    "socket.io": "4.7.4",
    "statuses": "2.0.1",
    "superagent": "10.1.1",
    "svg-captcha": "1.4.0",
    "triple-beam": "1.4.1",
    "typeorm": "0.3.20",
    "ua-parser-js": "1.0.38",
    "ulid": "2.3.0",
    "uuid": "11.0.2",
    "winston": "3.14.2",
    "winston-daily-rotate-file": "5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.1",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.3",
    "@types/bcrypt": "5.0.2",
    "@types/cookie-parser": "1.4.7",
    "@types/crypto-js": "4.2.2",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.2",
    "@types/lodash": "4.17.7",
    "@types/mime-types": "2.1.4",
    "@types/node": "22.10.7",
    "@types/pako": "2.0.3",
    "@types/statuses": "2.0.5",
    "@types/superagent": "8.1.9",
    "@types/supertest": "6.0.0",
    "@types/triple-beam": "1.3.5",
    "@types/ua-parser-js": "0.7.39",
    "cross-env": "7.0.3",
    "dotenv-cli": "8.0.0",
    "httpyac": "6.15.0",
    "jest": "29.5.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
